#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/arthurbr/test/node_modules/.pnpm/ts-jsdoc@3.2.2_typescript@5.0.4/node_modules/ts-jsdoc/out/node_modules:/home/arthurbr/test/node_modules/.pnpm/ts-jsdoc@3.2.2_typescript@5.0.4/node_modules/ts-jsdoc/node_modules:/home/arthurbr/test/node_modules/.pnpm/ts-jsdoc@3.2.2_typescript@5.0.4/node_modules:/home/arthurbr/test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/arthurbr/test/node_modules/.pnpm/ts-jsdoc@3.2.2_typescript@5.0.4/node_modules/ts-jsdoc/out/node_modules:/home/arthurbr/test/node_modules/.pnpm/ts-jsdoc@3.2.2_typescript@5.0.4/node_modules/ts-jsdoc/node_modules:/home/arthurbr/test/node_modules/.pnpm/ts-jsdoc@3.2.2_typescript@5.0.4/node_modules:/home/arthurbr/test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../out/ts2jsdoc.js" "$@"
else
  exec node  "$basedir/../../out/ts2jsdoc.js" "$@"
fi
